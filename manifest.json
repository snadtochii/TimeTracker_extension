{
  "name": "Time Tracker extention",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [ "https://m3s.materialise.net/*" ],
      "js": [ "js/jquery.js", "js/background.js" ],
	  "run_at": "document_end"
    }
  ],
    "permissions": [
        "storage"
    ],
  "web_accessible_resources": [
    "js/inject.js"
  ],
        "browser_action": {
        "default_popup": "index.html"
    }
}